{
  "name": "feedback-dashboard",
  "version": "1.0.0",
  "description": "Survey feedback dashboard application",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-client": "cd client && npm install",
    "install-all": "npm install && npm run install-client",
    "build:client": "cd client && npm run build",
    "build": "cd client && npm install && npm run build",
    "deploy": "node deploy.js",
    "deploy-prod": "node deploy.js --prod",
    "deploy-vercel": "vercel",
    "deploy-vercel-prod": "vercel --prod",
    "test-deployment": "node test-deployment.js",
    "git-setup": "git add . && git commit -m 'Deploy: Survey feedback dashboard' && git push"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "body-parser": "^1.20.2",
    "uuid": "^9.0.0",
    "express-rate-limit": "^6.8.0",
    "node-cache": "^5.1.2",
    "redis": "^4.6.7",
    "compression": "^1.7.4",
    "helmet": "^7.0.0",
    "express-mongo-sanitize": "^2.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1"
  }
}